{
  "name": "strellerminds_backend",
  "version": "0.0.1",
  "description": "StrellerMinds Backend - Minimal Blockchain Education Platform",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:api": "./scripts/newman-test.sh",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "audit:security": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "audit:fix-force": "npm audit fix --force",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/typeorm.config.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/typeorm.config.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/config/typeorm.config.ts",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/seed.command.ts",
    "backup": "bash scripts/backup.sh",
    "backup:restore": "bash scripts/restore.sh",
    "docs:generate": "./scripts/generate-docs.sh",
    "docs:serve": "npm run start:prod && echo 'API Docs available at http://localhost:3000/api/docs'",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "perf:test": "artillery run artillery-config.yml",
    "security:scan": "npm run audit:security && gitleaks detect --source . --verbose --report-path gitleaks-report.json",
    "quality:check": "npm run lint && npm run test:cov && npm run security:scan",
    "ci:test": "npm run test && npm run test:integration && npm run test:e2e && npm run test:api",
    "ci:build": "npm run build && npm run docs:generate"
  },
